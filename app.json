{
  "expo": {
    "name": "ecoapp",
    "slug": "ecoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/planeta.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/planeta.png",
        "backgroundColor": "transparent"
      },
      "package": "com.tcguus.ecoapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/planeta.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "07077f4d-8d76-49a7-b510-390e973a2465"
      }
    },
    "owner": "tcguus"
  }
}
