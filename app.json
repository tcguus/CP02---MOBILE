{
  "expo": {
    "name": "ecoapp",
    "slug": "ecoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/planeta.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/planeta.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tcguus.ecoapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/planeta.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "08c24678-4908-467c-a5f3-40e42d9ab798"
      }
    }
  }
}
